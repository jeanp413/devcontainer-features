{
    "name": "VS Code Browser",
    "id": "vscode-browser",
    "version": "1.0.2",
    "description": "Install VS Code Browser in an environment.",
    "options": {
    },
    "installsAfter": [
        "ghcr.io/devcontainers/features/common-utils"
